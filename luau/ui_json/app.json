{
  "root": {
    "tag": "div",
    "class": "w-full h-full flex items-center justify-center bg-neutral-950",
    "children": [
      {
        "card": {
          "tag": "div",
          "class": "px-6 py-3 rounded-xl border-2 border-blue-500 text-lg text-white cursor-pointer flex flex-col gap-2",
          "children": [
            {
              "button": {
                "tag": "button",
                "class": "bg-neutral-700 hover:bg-neutral-800 text-amber-200",
                "children": ["Hover me"]
              }
            },
            {
              "icon": {
                "tag": "image",
                "class": "w-16 h-16",
                "src": "assets/sprite.png"
              }
            },
            {
              "row": {
                "tag": "div",
                "class": "flex items-center gap-3",
                "children": [
                  {
                    "track": {
                      "tag": "div",
                      "class": "relative h-6 w-64 rounded-full bg-slate-800 rounded-full bg-red-500 border-2 border-black",
                      "children": [
                        {
                          "fill": {
                            "tag": "div",
                            "class": "h-full w-full transition-all duration-300 bg-green-500 rounded-full"
                          }
                        },
                        {
                          "label": {
                            "tag": "p",
                            "class": "absolute left-[50%] top-[50%] anchor-center",
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "tooltip": {
          "tag": "div",
          "class": "absolute px-3 py-2 rounded-lg bg-neutral-800 border border-neutral-600 text-sm text-white z-tooltip",
          "children": ["Anchored tooltip"]
        }
      }
    ]
  }
}
